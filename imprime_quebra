#include <stdio.h>

int imprime(int a, int n, int m) {

    if (a-1 == m){
        printf("\n");
    }
    if (a <= n){
        printf("%i ", a);
        return imprime(a+1, n, m);
    }
    

}

int main() {
  int n, m;
    printf("Digite um nÃºmero: ");
    scanf("%i %i", &n, &m);
  imprime(1, n, m);
}
